			prefHeight="25.0" prefWidth="332.0" />
		<Label layoutX="30.0" layoutY="89.0" text="Ip" />
		<Label fx:id="info" layoutX="40.0" layoutY="259.0"
			prefHeight="17.0" prefWidth="286.0" />
		<Label fx:id="exit" layoutX="8.0" layoutY="-25.0"
			prefHeight="73.0" prefWidth="351.0" textAlignment="CENTER"
			textFill="#f20303">
